[user]
	name = le0tk0k
	email = le0tk0k@gmail.com
[core]
	excludesfile = ~/.gitignore_global
[color]
	ui = auto
[color "status"]
	added = yellow
	changed = cyan
	untracked = red
[url "ssh://git@github.com"]
	insteadOf = https://github.com
[init]
	defaultBranch = main
[alias]
	st = status
	br = branch
	df = diff
	s = switch
	g = log --pretty='format:%C(yellow)%h %C(white)%s %C(red)%d %C(green)<%as,%an>' --graph
[pull]
	ff = only
	rebase = true
